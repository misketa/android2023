            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HomePageActivity"></activity>
        <activity android:name=".activity.RegistrationActivity"></activity>
        <activity android:name=".activity.FriendsListActivity"></activity>
        <activity android:name=".activity.ProfileActivity"></activity>
        <activity android:name=".activity.RangListActivity"></activity>
        <activity android:name=".activity.GamesNavigationActivity"></activity>
        <activity android:name=".activity.AssociationsActivity"></activity>
        <activity android:name=".activity.WhoKnowsActivity"></activity>
        <activity android:name=".activity.LinkItActivity"></activity>
    </application>

</manifest>